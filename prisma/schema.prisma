generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Jobs {
  id          Int      @id @default(autoincrement())
  name        String
  country     String
  wage        Decimal  @db.Decimal(10, 2)
  description String?  @db.VarChar(255)
  createdAt   DateTime @default(now())
}
